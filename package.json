{
  "name": "solar",
  "version": "0.0.1",
  "description": "Nebular admin frontend",
  "main": "bin/server.js",
  "scripts": {
    "dev": "NODE_ENV=development node ./node_modules/babel/bin/babel-node scripts/dev-server.js",
    "build": "node ./node_modules/better-npm-run build",
    "start": "node ./node_modules/better-npm-run start"
  },
  "betterScripts": {
    "build": {
      "command": "babel-node scripts/build.js",
      "env": {
        "NODE_ENV": "production",
        "FORCE_COLOR": "true"
      }
    },
    "start": {
      "command": "babel-node scripts/prod-server.js",
      "env": {
        "NODE_ENV": "production",
        "FORCE_COLOR": "true"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:nebulavision/solar.git"
  },
  "keywords": [
    "Nebular"
  ],
  "author": "RixTox <i@rix.li> (http://github.com/rixtox)",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "axios": "^0.7.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-typecheck": "^1.2.0",
    "babel-runtime": "^5.8.25",
    "better-npm-run": "0.0.2",
    "chalk": "^1.1.1",
    "compass-mixins": "^0.12.7",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.19.0",
    "dompurify": "^0.7.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "fs.extra": "^1.3.2",
    "history": "^1.11.0",
    "html-webpack-plugin": "^2.0.3",
    "lodash": "^3.10.1",
    "mime": "^1.3.4",
    "morgan": "^1.6.1",
    "node-sass": "^3.3.3",
    "normalizr": "^1.0.0",
    "react": "^0.13.3",
    "react-css-modules": "3.3.0",
    "react-redux": "^3.0.0",
    "react-router": "^1.0.0-rc1",
    "react-transform-hmr": "^1.0.0",
    "redux": "^3.0.0",
    "redux-devtools": "^2.1.3",
    "redux-devtools-diff-monitor": "^2.0.1",
    "redux-logger": "^2.0.0",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.0.0",
    "script-loader": "^0.6.1",
    "style-loader": "^0.12.4",
    "tinycolor2": "^1.1.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0",
    "wysihtml": "rixtox/wysihtml#build"
  }
}
