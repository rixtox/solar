{
  "name": "solar",
  "version": "0.0.1",
  "description": "Nebular admin frontend",
  "main": "bin/server.js",
  "scripts": {
    "dev": "node ./node_modules/better-npm-run dev",
    "build": "node ./node_modules/better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node scripts/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "FORCE_COLOR": "true"
      }
    },
    "build": {
      "command": "babel-node scripts/build.js",
      "env": {
        "NODE_ENV": "production",
        "FORCE_COLOR": "true"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:nebulavision/solar.git"
  },
  "keywords": [
    "Nebular"
  ],
  "author": "RixTox <i@rix.li> (http://github.com/rixtox)",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "babel-plugin-typecheck": "^1.2.0",
    "babel-runtime": "^5.8.24",
    "better-npm-run": "0.0.2",
    "chalk": "^1.1.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "fs.extra": "^1.3.2",
    "html-webpack-plugin": "^2.0.3",
    "morgan": "^1.6.1",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-transform-hmr": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  }
}
